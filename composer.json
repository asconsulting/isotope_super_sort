{
    "name":"asconsulting/isotope_super_sort",
    "description":"Customized sorting for Isotope eCommerce",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "isotope", "ecommerce", "sorting"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.4",
        "contao/core-bundle": ">=4.9",
		"isotope/isotope-core": ">=2.6"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SuperSort\\": [
                "system/modules/isotope_super_sort/library/SuperSort"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/isotope_super_sort": "system/modules/isotope_super_sort"
            }
        }
    },
    "minimum-stability": "dev"
}